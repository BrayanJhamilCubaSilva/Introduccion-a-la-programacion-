/*Descripción: Ingresar un rango determinado de numeros enteros positivos*/
/*Entrada    : El rango de numeros enteros*/
/*Salida     : Los numeros primos y su suma */
/******************************************************************************************/


#include "stdafx.h"
#include "stdio.h"
#include "conio.h"

void main()
{int n, primos, i;
 printf("Ingrese un rango determinado de numeros primos:");
 scanf_s("%d", &n);
 primos = 0;
 for (i=1; i<=n; i++)
 {
   printf("%d\n",i);
	 if((i/i==1)&&(i/1==i))
	 {
	   
	   printf("Los numero primos son:%d\n",i);
	  
	 }
	  primos = primos + i;
	 
 }
  printf("La suma de primos es:%d",primos);
 _getch();
}
